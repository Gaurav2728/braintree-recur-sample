<!DOCTYPE html>
<html>
<head>
  <title>BraintreeRecurSample</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>

<%- flash.each do |name, msg| -%>
  <%= content_tag :div, msg, :id => "flash_#{name}" if msg.is_a?(String) %>
<%- end -%>

<p><%= link_to 'Home', root_path %></p>


<%= yield %>

</body>
</html>
